type Result_1 = 
 variant {
   err: text;
   ok: nat;
 };
type Result = 
 variant {
   err: text;
   ok;
 };
type Post = 
 record {
   author: text;
   content: text;
   id: nat;
   timestamp: int;
   title: text;
 };
service : {
  createPost: (text, text, text) -> (Result_1);
  deletePost: (nat) -> (Result);
  editPost: (nat, text, text, text) -> (Result);
  getFeaturedPosts: () -> (vec Post) query;
  getPosts: () -> (vec Post) query;
  setFeaturedPost: (nat) -> (Result);
}
